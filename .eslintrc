{
  env: {
    es6: true,
    node: true,
    mocha: true,
    browser: true,
  },
  "plugins": [
    "promise"
  ],
  extends: [
    "airbnb-base",
    "plugin:promise/recommended"
  ],
  parserOptions: {
    ecmaVersion: 2016,
    sourceType: "module",
  },
  "globals": {
    "YT": true,
  },
  "settings": {
    "import/core-modules": [
      electron,
      electron-installer-windows
      ]
  },
  rules: {
    "no-new": "off",
    "new-cap": "off",
    "max-len": ["error", 120],
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-console": "off",
    "func-names": "off",
    "class-methods-use-this": "off",
    "prefer-destructuring": ["error", {"array": false}]
  }
}
