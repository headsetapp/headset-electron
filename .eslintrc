{
  env: {
    es6: true,
    node: true,
    mocha: true,
    browser: true,
  },
  "plugins": [
    "promise"
  ],
  extends: [
    "airbnb-base",
    "plugin:promise/recommended"
  ],
  parser: "babel-eslint",
  parserOptions: {
    ecmaVersion: 6,
    sourceType: "module",
    ecmaFeatures: {
      experimentalObjectRestSpread: true,
    }
  },
  "globals": {
    "YT": true,
  },
  "settings": {
    "import/core-modules": [
      "electron",
      electron-winstaller
      ]
  },
  rules: {
    "no-new": "off",
    "new-cap": "off",
    "max-len": ["error", 120],
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-console": "off",
    "func-names": "off",
    "class-methods-use-this": "off",
    "prefer-destructuring": ["error", {"array": false}],
    "promise/no-callback-in-promise": "off"
  }
}
